# Alpaca Trading API Configuration
# Get your API keys from: https://app.alpaca.markets/paper/dashboard/overview

# Alpaca API Keys
NEXT_PUBLIC_ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here

# Trading Mode (set to 'true' for paper trading, 'false' for live trading)
NEXT_PUBLIC_ALPACA_PAPER_TRADING=true

# MooMoo Configuration (existing)
NEXT_PUBLIC_MOOMOO_API_URL=http://localhost:11111
NEXT_PUBLIC_MOOMOO_ENABLED=true

# Database Configuration (existing)
DATABASE_URL=your_database_url_here

# Other existing environment variables...
